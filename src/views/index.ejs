<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/styles.css">
    <title>Document</title>
</head>
<body>
    <%- include("./nav")  %>
    <div class="messages">
        <% messages.map((msg, index) => { %>
            <div>
                <p><%= msg.user %></p>
                <p><%= msg.added %></p>
                <p><%= msg.text %></p>
                <button><a href="/messages/<%= index %>">Open</a></button>
            </div>
        <% }) %>
    </div>
</body>
</html>